<mvc:View
   controllerName="bsim.hcmapp.man.movement.controller.Acting"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns="sap.m"
   xmlns:f="sap.ui.layout.form"
   xmlns:l="sap.ui.layout"
   xmlns:core="sap.ui.core"
   xmlns:semantic="sap.m.semantic">
<semantic:FullscreenPage navButtonPress="onNavBack" showNavButton="true" title="{i18n>employeeActing}">
    <semantic:content>
    <VBox class="sapUiSmallMargin">
        <!-- Employee Data Panel -->
        <Panel 
        expandable="true" 
        expanded="false"
        headerText="Employee Data" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                id="employeeActingForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="{i18n>name}"/>
                            <HBox>
                                <Input value="{Acting>employee>/EmployeeName/FormattedName}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>entryDate}"/>
                            <HBox>
                                <Input value="{Acting>employee>/HiringDate}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA1}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA2}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA3}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>marketingAvg}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Sanksi Aktif Panel -->
        <Panel 
        expandable="true" 
        expanded="false"
        headerText="Sanksi Aktif" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                id="sanksiActingForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="{i18n>sanctionType}"/>
                            <l:Grid vSpacing="0" containerQuery="true">
                            <Input id="sanctionTypeActing" value="{Acting>employee>/SanksiAktif}" width="auto" editable="false" >
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <VBox>
                            <Text id="sanctionDescriptionActing" text="Surat Teguran Surat" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                                <Button
                                    icon="sap-icon://document-text" 
                                    text="Show Document"
                                    type="Emphasized"
                                    tooltip="View Document"
                                    press="onDisplayDocumentWarning" class="sapUiTinyMarginBegin"/>
                            </VBox>
                            </l:Grid>
                        <Label text="Jenis Pelanggaran" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel1Acting" value="{Acting>employee>/JenPel1}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel1Acting" text="{Acting>employee>/DescPel1}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel2Acting" value="{Acting>employee>/JenPel2}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel2Acting" text="{Acting>employee>/DescPel2}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel3Acting" value="{Acting>employee>/JenPel3}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel3Acting" text="{Acting>employee>/DescPel3}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel4Acting" value="{Acting>employee>/JenPel4}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel4Acting" text="{Acting>employee>/DescPel4}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="{i18n>actDate}" required="false"/>
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <DatePicker id="actDateStartActing" value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                                <Text text="-" class="sapUiTinyMarginEnd"/>
                                <DatePicker id="actDateEndActing" value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                            </HBox>
                        <Label text="information" visible="false"/>
                        <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop" width="57%">
                        <MessageStrip
                            text="Yang bersangkutan sedang dalam proses pengajuan [ST/TP] yang diajukan padaa tanggal dd.mm.yyyy"
                            type="Warning"
                            showIcon="true"
                            showCloseButton="false"
                            class="sapUiMediumMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Action Request Panel -->
        <Panel 
        expandable="true" 
        expanded="true"
        headerText="Action Request" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                id="actionActingForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="{i18n>actionType}" />
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <Input id="actionTypeActing" value="{Acting>employee>/ActionType}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>reason}" />
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <Input id="reasonActing" value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="true"/>
                            </HBox>
                        <Label text="{i18n>effectiveDate}" />
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <DatePicker id="effectiveDateStartActing" value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                                <Text text="-" class="sapUiTinyMarginEnd"/>
                                <DatePicker id="effectiveDateEndActing" value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                            </HBox>
                        <Label text="{i18n>mutation}" visible="false"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                                <Text text="Dari" class="sapUiLargeMarginEnd"/>
                                <Text id="mutationFromActing" text="" class="sapUiLargeMarginBeginEnd" />
                                <Text id="mutationToActing" text="" class="sapUiTinyMarginBeginEnd" />
                                <Text text="Menjadi" class="sapUiLargeMarginBegin"/>
                            </HBox>
                        <Label text="{i18n>positionId}" required="true" />
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPositionIdActing" value="{Acting>employee>/EmployeePositionId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPositionIdActing" value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="{i18n>positionText}" required="true" />
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPositionTextActing" value="{Acting>employee>/EmployeePositionText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPositionTextActing" value="{Acting>employee>/EmployeePositionText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>perAreaId}" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPerAreaIdActing" value="{Acting>employee>/PersonnelAreaId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPerAreaIdActing" value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="{i18n>perAreaText}" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPerAreaTextActing" value="{Acting>employee>/PersonnelAreaText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPerAreaTextActing" value="{Acting>employee>/PersonnelAreaText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>perSubAreaId}" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPerSubAreaIdActing" value="{Acting>employee>/PersonnelSubareaId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPerSubAreaIdActing" value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="{i18n>perSubAreaText}" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPerSubAreaTextActing" value="{Acting>employee>/PersonnelSubareaText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPerSubAreaTextActing" value="{Acting>employee>/PersonnelSubareaText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>unitOrgId}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentUnitOrgIdActing" value="{Acting>employee>/EmployeeOrgunitId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newUnitOrgIdActing" value="{Acting>employee>/EmployeeOrgunitId}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>unitOrgText}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentUnitOrgTextActing" value="{Acting>employee>/EmployeeOrgunitText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newUnitOrgTextActing" value="{Acting>employee>/EmployeeOrgunitText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>divisionId}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentDivisionIdActing" value="{Acting>employee>/DivisionId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newDivisionIdActing" value="{Acting>employee>/DivisionId}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>divisionText}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentDivisionTextActing" value="{Acting>employee>/DivisionText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newDivisionTextActing" value="{Acting>employee>/DivisionText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>positionMPP}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPositionMPPActing" value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPositionMPPActing" value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>grade}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentGradeActing" value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newGradeActing" value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>endDateJob}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="EndDateActing" value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>salaryAdj}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Select id="salaryAdjActing" required="true" fieldGroupIds="grpValidation"
                                items="{ path: 'dropdown>/salaryAdjColl', sorter: { path: 'text' }}"
                                forceSelection="false"
                                selectedKey="{dropdown>/selectedSalaryAdj}"
                                width="11.5rem">
                                <items>
                                    <core:Item key="{dropdown>key}" text="{dropdown>text}" />
                                </items>
                            </Select>
                            </HBox>
                        <Label text="{i18n>salaryAdjValue}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="salaryAdjValueActing" value="" width="100%" class="sapUiTinyMarginEnd" editable="true"/>
                            </HBox>
                        <Label text="{i18n>as}" required="true"/>
                            <Select id="asActing" required="true" fieldGroupIds="grpValidation"
                                items="{ path: 'dropdown>/asColl', sorter: { path: 'text' }}"
                                forceSelection="false"
                                selectedKey="{dropdown>/selectedAs}"
                                width="11.5rem"
                                class="sapUiSmallMarginBegin">
                                <items>
                                    <core:Item key="{dropdown>key}" text="{dropdown>text}" />
                                </items>
                            </Select>
                        <Label text="{i18n>employeeChange}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="employeeChangeActing" value="" width="100%" class="sapUiTinyMarginEnd" editable="true"/>
                            </HBox>
                        <Label text="{i18n>basicCon}" />
                            <VBox class="sapUiSmallMarginBeginEnd">
                            <TextArea 
                                id="basicConActing"
                                value="" 
                                width="57%"
                                rows="4"
                                showExceededText="true" 
                                maxLength="255" 
                                liveChange="handleLiveChange" 
                                valueState="Warning" 
                                valueLiveUpdate="true"
                                required="true" 
                                fieldGroupIds="grpValidation"
                                class="sapUiTinyMarginEnd" 
                                editable="true"/>
                            </VBox>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginBegin" width="57%">
                        <MessageStrip
                            text="{i18n>warningEvaluation}"
                            type="Information"
                            showIcon="true"
                            class="sapUiTinyMarginBottom">
                        </MessageStrip>
                        <MessageStrip
                            text="{i18n>warningPKWT}"
                            type="Information"
                            showIcon="true"
                            class="sapUiMediumMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Verification -->
        <Panel 
        expandable="true" 
        expanded="true"
        headerText="Verification" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                editable="true"
                id="assessmentActingForm"
                layout="ResponsiveGridLayout" 
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Toolbar id="AssessmentActing">
                            <Text text="{i18n>assessment}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="{i18n>assessmentResult}" required="true"/>
                            <HBox>
                                <Input id="assessmentResultActing" value="No data, harap upload hasil assessment" width="18rem" class="sapUiMediumMarginEnd" editable="false"/>
                                <CheckBox id="verifyResultActing" text="Verify" selected="{cbResultActing>/verifyResultActing}"/>
                            </HBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                editable="true"
                id="biCheckingActingForm"
                layout="ResponsiveGridLayout" 
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Toolbar id="BICheckingActing">
                            <Text text="{i18n>biChecking}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="{i18n>biResult}" required="true"/>
                        <HBox class="sapUiSmallMarginEnd">
                            <Text text="Collection:" class="sapUiSmallMarginEnd" />
                            <CheckBox id="hasilActing1" text="1" selected="{hasilActing>/hasilActing1}"/>
                            <CheckBox id="hasilActing2" text="2" selected="{hasilActing>/hasilActing2}"/>
                            <CheckBox id="hasilActing3" text="3" selected="{hasilActing>/hasilActing3}"/>
                            <CheckBox id="hasilActing4" text="4" selected="{hasilActing>/hasilActing4}"/>
                            <CheckBox id="hasilActing5" text="5" selected="{hasilActing>/hasilActing5}"/>
                        </HBox>
                        <Label text="{i18n>biNotes}" />
                        <TextArea 
                            id="hasilBiCheckingActing"
                            value=""
                            width="57%"
                            rows="4"
                            showExceededText="true" 
                            maxLength="255" 
                            liveChange="handleLiveChange" 
                            valueState="Warning" 
                            valueLiveUpdate="true"
                            editable="true"/>
                        <Label text="informasi" visible="false"/>
                        <VBox width="57%">
                        <MessageStrip
                            text="{i18n>warningBiChecking}"
                            type="Information"
                            showIcon="true"
                            class="sapUiTinyMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                editable="true"
                id="dispoActingForm"
                layout="ResponsiveGridLayout" 
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Toolbar id="disposisiToolbarActing">
                            <Text text="{i18n>disposisi}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="{i18n>rekomendasiHCM}" />
                        <TextArea 
                            id="rekomendasiHCMActing"
                            value=""
                            width="57%"
                            rows="4"
                            showExceededText="true" 
                            maxLength="255" 
                            liveChange="handleLiveChange" 
                            valueState="Warning" 
                            valueLiveUpdate="true"
                            editable="true"/>
                        <Label text="{i18n>downloadDis}" />
                        <HBox class="sapUiSmallMarginEnd">
                            <Button
                                icon="sap-icon://download"
                                text="Download"
                                type="Emphasized"
                                press="onDownloadDisposisi"
                                class="sapUiTinyMarginEnd"/>
                        </HBox>
                        <Label text="{i18n>disResult}" required="true"/>
                        <RadioButtonGroup id="disposisiActing" columns="2" width="100%" selectedIndex="{disposisiStatus>/selectedIndex}">
                            <RadioButton text="Rekomendasi" class="sapUiSmallMarginEnd"/>
                            <RadioButton text="Tidak Direkomendasikan" />
                        </RadioButtonGroup>
                        <Label text="{i18n>dispoNote}" />
                        <TextArea 
                            id="dispoNoteActing"
                            value=""
                            width="57%"
                            rows="4"
                            showExceededText="true" 
                            maxLength="255" 
                            liveChange="handleLiveChange" 
                            valueState="Warning" 
                            valueLiveUpdate="true"
                            editable="true"/>
                        <Label text="informasi" visible="false"/>
                        <VBox width="57%">
                        <MessageStrip
                            text="{i18n>warningDisposisi}"
                            type="Information"
                            showIcon="true"
                            class="sapUiTinyMarginBottom">
                        </MessageStrip>
                        </VBox>
                        <Label text="{i18n>salaryAdj}" />
                        <HBox>
                        <Text text="Rp" />
                        <Input 
                            id="gajiActing"
                            type="Number"
                            value=""
                            width="85%"
                            maxLength="50"
                            editable="true"/>
                        </HBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                editable="true"
                id="upDocActingForm"
                layout="ResponsiveGridLayout" 
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Toolbar id="upDocActing">
                            <Text text="{i18n>upDoc}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <VBox class="sapUiSmallMarginEnd">
                        <HBox alignItems="Center" justifyContent="End">
                        <Button
                            width="5rem"
                            icon="sap-icon://add"
                            type="Transparent"
                            tooltip="Add Item"
                            press=".onOpenDialog"/>
                        </HBox>
                        <HBox >
                        <Table
                            id="table-uploadSet"
                            width="70rem"
                            mode="MultiSelect"
                            fixedLayout="false"
                            inset="true"
                            autoPopinMode="true"
                            hiddenInPopin="None"
                            items="{path: 'docUploads>/uploads', templateShareable: false}"                                
                            selectionChange="onSelectionChange">
                            <columns>
                                <Column id="namaDokumen" importance="High">
                                    <header>
                                        <Label text="Nama Dokumen" />
                                    </header>
                                </Column>
                                <Column id="jenisDokumen">
                                    <header>
                                        <Label text="Jenis Dokumen" />
                                    </header>
                                </Column>
                                <Column id="pic">
                                    <header>
                                        <Label text="PIC" />
                                    </header>
                                </Column>
                                <Column id="keterangan">
                                    <header>
                                        <Label text="Keterangan" />
                                    </header>
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem
                                    highlight="{= ${documents>uploadState} === 'Complete' ? 'Indication05' : null }">

                                     <!-- Cells with cell template defining content for each column (cells and columns work with 1:1 mapping) -->
                                    <cells>
                                    <HBox>
                                        <Image
                                            src="{documents>imageUrl}"
                                            class="sapMUSTItemImage sapMUSTItemIcon"
                                            visible="{= ${documents>imageUrl} !== undefined}"/>
                                        <core:Icon
                                            src="sap-icon://pdf-attachment"
                                            color="white"
                                            class="sapMUSTItemImage sapMUSTItemIcon"/>
                                        <VBox
                                            class="sapUiTinyMargin sapUiSmallMarginBegin">
                                            <Link
                                                text="{docUploads>fileName}"
                                                press="onShowDoc"
                                                class="sapUiTinyMarginBottom"/>
                                            <Text text="{path: 'docUploads>fileSize', formatter: '.formatter.formatFileSize'}" />
                                        </VBox>
                                    </HBox>
                                    <Text text="{docUploads>docTypeText}" />
                                    <Text text="{docUploads>pic}" />
                                    <Text
                                        text="{docUploads>keterangan}" 
                                        wrapping="true"/>
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                        </HBox>                  
                        </VBox> 
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
    </VBox>
    </semantic:content>
    <semantic:mainAction>
        <semantic:MainAction id="btnSubmitActing" text="Submit" press="onSendRequest"/>
    </semantic:mainAction>
</semantic:FullscreenPage>
</mvc:View>