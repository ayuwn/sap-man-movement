<mvc:View
   controllerName="bsim.hcmapp.man.movement.controller.Mutation"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns="sap.m"
   xmlns:f="sap.ui.layout.form"
   xmlns:l="sap.ui.layout"
   xmlns:core="sap.ui.core"
   xmlns:semantic="sap.m.semantic">
<semantic:FullscreenPage navButtonPress="onNavBack" showNavButton="true" title="{i18n>employeeStatusChange}">
    <semantic:content>
    <VBox class="sapUiSmallMargin">
        <!-- Employee Data Panel -->
        <Panel 
        expandable="true" 
        expanded="false"
        headerText="Employee Data" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                id="employeeDataForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="Nama"/>
                            <HBox>
                                <Input value="{employee>/EmployeeName/FormattedName}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="Tanggal Masuk"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="Rating PA 1"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="Rating PA 2"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="Rating PA 3"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="Pencapaian Marketing (Avg)"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Sanksi Aktif Panel -->
        <Panel 
        expandable="true" 
        expanded="false"
        headerText="Sanksi Aktif" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                id="sanksiAktifForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="Jenis Sanksi"/>
                            <l:Grid vSpacing="0" containerQuery="true">
                            <Input value="" width="auto" editable="false" >
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <VBox>
                            <Text text="Surat Teguran Surat" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                                <Button
                                    icon="sap-icon://document-text" 
                                    text="Show Document"
                                    type="Emphasized"
                                    tooltip="View Document"
                                    press="onDisplayDocumentWarning" class="sapUiTinyMarginBegin"/>
                            </VBox>
                            </l:Grid>
                        <Label text="Jenis Pelanggaran" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input value="{employee>/JenPel1}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employee>/DescPel1}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input value="{employee>/JenPel2}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employee>/DescPel2}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input value="{employee>/JenPel3}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employee>/DescPel3}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input value="{employee>/JenPel4}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employee>/DescPel4}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="Tanggal Berlaku" required="false"/>
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <DatePicker value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                                <Text text="-" class="sapUiTinyMarginEnd"/>
                                <DatePicker value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                            </HBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Action Request Panel -->
        <Panel 
        expandable="true" 
        expanded="true"
        headerText="Action Request" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                id="actionRequestForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="Action Type" />
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd"/>
                            </HBox>
                        <Label text="Reason for Action" />
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd"/>
                            </HBox>
                        <Label text="Tanggal Efektif" />
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <DatePicker value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                                <Text text="-" class="sapUiTinyMarginEnd"/>
                                <DatePicker value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                            </HBox>
                        <Label text="Mutasi" visible="false"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                                <Text text="Dari" class="sapUiLargeMarginEnd"/>
                                <Text text="" class="sapUiLargeMarginBeginEnd" />
                                <Text text="" class="sapUiTinyMarginBeginEnd" />
                                <Text text="Menjadi" class="sapUiLargeMarginBegin"/>
                            </HBox>
                        <Label text="Posisi" required="true" />
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="{employee>/EmployeePositionText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="Kantor (Personnel Area)" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="{employee>/PersonnelAreaText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="Cabang (Personnel Subarea)" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="{employee>/PersonnelSubareaText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="Status" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="{employee>/EmployeeOrgunitText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="Contract Type"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="Jabatan" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="{employee>/EmployeeJobtitleLongtext}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="Homebase"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="Pangkat/Level"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="{employee>/PayScaleLevel}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="{employee>/PayScaleLevel}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="Unit Organisasi"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="{employee>/EmployeeOrgunitText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="{employee>/EmployeeOrgunitText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="Posisi Atasan Langsung"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="MPP sesuai Posisi"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="Jumlah Karyawan Existing sesuai Posisi"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="Jumlah Vacant sesuai Posisi"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="Karyawan Existing sesuai Posisi"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="Bawahan Langsung sesuai Posisi"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="Status Jabatan"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="Tanggal Berakhir Status Jawaban"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            </HBox>
                        <Label text="Penyesuaian Gaji"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="true" required="true"/>
                            <Text text="Rp" class="sapUiMediumMarginBegin" />
                            <Input value="" width="85%" class="sapUiLargeMarginEnd" editable="true" required="false"/>
                            </HBox>
                        <Label text="Sebagai" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input value="" width="100%" class="sapUiTinyMarginEnd" editable="true"/>
                            </HBox>
                        <Label text="Dasar Pertimbangan" />
                            <VBox class="sapUiSmallMarginBeginEnd">
                            <TextArea 
                            value="" 
                            width="57%"
                            rows="4"
                            maxLength="2000" 
                            class="sapUiTinyMarginEnd" 
                            editable="true"/>
                            </VBox>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginBegin sapUiSmallMarginTop" width="57%">
                        <MessageStrip
                            text="Harap Lampirkan hasil Evaluasi di bagian Upload Dokumen"
                            type="Information"
                            showIcon="true"
                            class="sapUiTinyMarginBottom">
                        </MessageStrip>
                        <MessageStrip
                            text="Harap Lampirkan PKWT terakhir (jika perpanjang kontrak) di bagian Upload Dokummen"
                            type="Information"
                            showIcon="true"
                            class="sapUiMediumMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Verification -->
        <Panel 
        expandable="true" 
        expanded="true"
        headerText="Verification" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                editable="true"
                id="assessmentnForm"
                layout="ResponsiveGridLayout" 
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Toolbar id="Assessment">
                            <Text text="Assessment (Diisi team Talent dan Management)" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="Hasil Assessment" required="true"/>
                        <RadioButtonGroup columns="2" width="100%">
                            <RadioButton id="RB1" text="Melebihi kualifikasi" />
                            <RadioButton id="RB2" text="Memenuhi kualifikasi" />
                            <RadioButton id="RB3" text="Hampir memenuhi kualifikasi" />
                            <RadioButton id="RB4" text="Belum memenuhi kualifikasi" />
                        </RadioButtonGroup>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginTop" width="57%">
                        <MessageStrip
                            text="Harap Lampirkan hasil Assessment di bagian Upload Dokumen"
                            type="Information"
                            showIcon="true"
                            class="sapUiTinyMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                editable="true"
                id="biForm"
                layout="ResponsiveGridLayout" 
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Toolbar id="BI">
                            <Text text="BI Checking (Diisi Compensation dan Benefit)" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="Hasil BI Checking" required="true"/>
                        <HBox class="sapUiSmallMarginEnd">
                        <Text text="Collection:" class="sapUiSmallMarginEnd" />
                        <CheckBox id="hasil1" text="1" />
                        <CheckBox id="hasil2" text="2" />
                        <CheckBox id="hasil3" text="3" />
                        <CheckBox id="hasil4" text="4" />
                        <CheckBox id="hasil5" text="5" />
                        </HBox>
                        <Label text="Catatan BI Checking" />
                        <TextArea 
                        id="hasilBiChecking"
                        value=""
                        width="57%"
                        rows="4"
                        maxLength="2000"
                        editable="true"/>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginTop" width="57%">
                        <MessageStrip
                            text="Harap Lampirkan hasil BI Checking di bagian Upload Dokumen"
                            type="Information"
                            showIcon="true"
                            class="sapUiTinyMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                editable="true"
                id="disposisiForm"
                layout="ResponsiveGridLayout" 
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Toolbar id="disposisiTb">
                            <Text text="Disposisi (Diisi Data Management)" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="Download file template Disposisi (excel)" />
                        <HBox class="sapUiSmallMarginEnd">
                            <Button
                                icon="sap-icon://download"
                                text="Download"
                                type="Emphasized"
                                press="onDownloadDisposisi"
                                class="sapUiTinyMarginEnd"/>
                        </HBox>
                        <Label text="Hasil Disposisi" required="true"/>
                        <RadioButtonGroup columns="2" width="100%">
                            <RadioButton id="Dis1" text="Disetujui" class="sapUiSmallMarginEnd"/>
                            <RadioButton id="Dis2" text="Tidak Disetujui" />
                        </RadioButtonGroup>
                        <Label text="Penyesuaian Gaji" />
                        <HBox>
                        <Text text="Rp" />
                        <Input 
                        id="gaji"
                        type="Number"
                        value=""
                        width="85%"
                        maxLength="50"
                        editable="true"/>
                        </HBox>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginTop" width="57%">
                        <MessageStrip
                            text="Harap Lampirkan hasil Disposisi di bagian Upload Dokumen"
                            type="Information"
                            showIcon="true"
                            class="sapUiTinyMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                editable="true"
                id="upDokumenForm"
                layout="ResponsiveGridLayout" 
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Toolbar id="upDokumen">
                            <Text text="Upload Dokumen" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <VBox class="sapUiSmallMarginEnd">
                        <HBox alignItems="Center" justifyContent="End">
                        <Button
                            width="5rem"
                            icon="sap-icon://add"
                            type="Transparent"
                            tooltip="Add Item"
                            press=".onOpenDialog"/>
                        </HBox>
                        <HBox >
                        <Table>
                            <columns>
                                <Column width="100px">
                                    <Text text="Jenis Dokumen" />
                                </Column>
                                <Column  width="120px">
                                    <Text text="PIC" />
                                </Column>
                                <Column  width="250px">
                                    <Text text="Keterangan" />
                                </Column>
                                <Column width="80px">
                                    <Text text="Upload" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <VBox> 
                                        <Text text="Evaluasi Karyawan" />
                                        </VBox>
                                        <VBox>
                                        <Text text="Requestor" />
                                        </VBox>
                                        <VBox> 
                                        <Text text="" />
                                        </VBox>
                                        <VBox> 
                                            <Button 
                                                icon="sap-icon://document-text" 
                                                text="Show Document"
                                                type="Emphasized"
                                                tooltip="View Document"
                                                press="onDisplayDocumentWarning"/>
                                        </VBox>
                                    </cells>
                                </ColumnListItem>
                                <ColumnListItem>
                                    <cells>
                                        <VBox> 
                                        <Text text="PKWT Terakhir" />
                                        </VBox>
                                        <VBox>
                                        <Text text="Requestor" />
                                        </VBox>
                                        <VBox> 
                                        <Text text="Khusus Contract Extend" />
                                        </VBox>
                                        <VBox> 
                                            <Button 
                                                icon="sap-icon://document-text" 
                                                text="Show Document"
                                                type="Emphasized"
                                                tooltip="View Document"
                                                press="onDisplayDocumentWarning"/>
                                        </VBox>
                                    </cells>
                                </ColumnListItem>
                                <ColumnListItem>
                                    <cells>
                                        <VBox> 
                                        <Text text="Assessment" />
                                        </VBox>
                                        <VBox>
                                        <Text text="Talent and Management" />
                                        </VBox>
                                        <VBox> 
                                        <Text text="" />
                                        </VBox>
                                        <VBox> 
                                            <Button 
                                                icon="sap-icon://document-text" 
                                                text="Show Document"
                                                type="Emphasized"
                                                tooltip="View Document"
                                                press="onDisplayDocumentWarning"/>
                                        </VBox>
                                    </cells>
                                </ColumnListItem>
                                <ColumnListItem>
                                    <cells>
                                        <VBox> 
                                        <Text text="Hasil BI Checking" />
                                        </VBox>
                                        <VBox>
                                        <Text text="Compensation and Benefit" />
                                        </VBox>
                                        <VBox> 
                                        <Text text="" />
                                        </VBox>
                                        <VBox> 
                                            <Button 
                                                icon="sap-icon://document-text" 
                                                text="Show Document"
                                                type="Emphasized"
                                                tooltip="View Document"
                                                press="onDisplayDocumentWarning"/>
                                        </VBox>
                                    </cells>
                                </ColumnListItem>
                                <ColumnListItem>
                                    <cells>
                                        <VBox> 
                                        <Text text="Disposisi" />
                                        </VBox>
                                        <VBox>
                                        <Text text="Data Management" />
                                        </VBox>
                                        <VBox> 
                                        <Text text="" />
                                        </VBox>
                                        <VBox> 
                                            <Button 
                                                icon="sap-icon://document-text" 
                                                text="Show Document"
                                                type="Emphasized"
                                                tooltip="View Document"
                                                press="onDisplayDocumentWarning"/>
                                        </VBox>
                                    </cells>
                                </ColumnListItem>
                                <ColumnListItem>
                                    <cells>
                                        <VBox> 
                                        <Text text="PKWT Baru" />
                                        </VBox>
                                        <VBox>
                                        <Text text="Requestor" />
                                        </VBox>
                                        <VBox> 
                                        <Text text="Yang sudah ditanda-tangani pegawai" />
                                        </VBox>
                                        <VBox> 
                                            <Button 
                                                icon="sap-icon://document-text" 
                                                text="Show Document"
                                                type="Emphasized"
                                                tooltip="View Document"
                                                press="onDisplayDocumentWarning"/>
                                        </VBox>
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                        </HBox>                  
                        </VBox> 
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
    </VBox>
    </semantic:content>
    <semantic:mainAction>
        <semantic:MainAction id="btnSubmitMutation" text="Submit" press="onSubmit"/>
    </semantic:mainAction>
</semantic:FullscreenPage>
</mvc:View>