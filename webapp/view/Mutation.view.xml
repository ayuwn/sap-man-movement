<mvc:View
   controllerName="bsim.hcmapp.man.movement.controller.Mutation"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns="sap.m"
   xmlns:f="sap.ui.layout.form"
   xmlns:l="sap.ui.layout"
   xmlns:core="sap.ui.core"
   xmlns:semantic="sap.m.semantic"
   xmlns:vm="sap.ui.fl.variants"
   xmlns:mdc="sap.ui.mdc"
   xmlns:mdct="sap.ui.mdc.table"
   xmlns:upload="sap.m.upload" 
   xmlns:u="sap.ui.unified"
   xmlns:c="sap.f">
<semantic:FullscreenPage id="Movement" navButtonPress="onNavBack" showNavButton="true" title="{i18n>appMovement}">
    <semantic:content>

        <!-- Profile -->
        <VBox class="sapUiMediumMargin" alignItems="Center" justifyContent="Center">
            <HBox backgroundDesign="Solid" width="30%" id="profile" class="roundedProfile">
                <HBox class="sapUiSmallMarginBegin sapUiTinyMarginEndBottomTop" alignItems="Center" justifyContent="Center">
                    <Avatar displaySize="Custom"
                            initials="{path: 'employeeDetail>/EmployeeName/FormattedName', formatter: '.formatter.getEmployeeInitial'}" backgroundColor="Random" customDisplaySize="3rem"
                            tooltip="{employeeDetail>/EmployeeName/FormattedName}" class="sapMTableContentMargin sapUiSmallMarginTop"/>
                    <VBox class="sapUiSmallMarginBegin">
                        <ObjectIdentifier title="{employeeDetail>/EmployeeName/FormattedName} ({employee>/EmployeeNumber})" class="sapUiTinyMarginBottom"/>
                        <Text text="{employeeDetail>/EmployeePositionText}" />
                    </VBox>
                </HBox>
            </HBox>
        </VBox>       

        <!-- Employee Data Panel -->
        <Panel 
        expandable="true" 
        expanded="false"
        headerText="Employee Data" 
        width="auto">
            <content>
                <f:SimpleForm 
                id="employeeMutationForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="{i18n>entryDate}"/>
                            <HBox>
                                <Input id="entryDateMutation" value="{path: 'employeeDetail>/HiringDate', formatter: '.formatter.formatDateDisplay'}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA1}"/>
                            <HBox>
                                <Input id="ratingPA1Mutation" value="{employeeDetail>/Rat01}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA2}"/>
                            <HBox>
                                <Input id="ratingPA2Mutation" value="{employeeDetail>/Rat02}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA3}"/>
                            <HBox>
                                <Input id="ratingPA3Mutation" value="{employeeDetail>/Rat03}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>marketingAvg}"/>
                            <HBox>
                                <Input id="marketingAvgMutation" value="{path: 'employeeDetail>/Avg', formatter: '.formatter.formatAvg'}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Sanksi Aktif Panel -->
        <Panel 
        expandable="true" 
        expanded="false"
        headerText="Sanksi Aktif" 
        width="auto">
            <content>
                <f:SimpleForm 
                id="sanksiMutationForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="{i18n>sanctionType}"/>
                            <l:Grid vSpacing="0" containerQuery="true">
                            <Input id="sanctionTypeMutation" value="{employeeDetail>/SanksiAktif}" width="auto" editable="false" >
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <VBox>
                            <Text id="sanctionDescriptionMutation" text="Surat Teguran" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                                <Button
                                    icon="sap-icon://document-text" 
                                    text="Show Document"
                                    type="Emphasized"
                                    tooltip="View Document"
                                    press="onDisplayDocumentWarning" class="sapUiTinyMarginBegin"/>
                            </VBox>
                            </l:Grid>
                        <Label text="Jenis Pelanggaran" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel1Mutation" value="{employeeDetail>/JenPel1}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel1Mutation" text="{path: 'employeeDetail>/DescPel1', formatter: '.formatter.formatDescPel'}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel2Mutation" value="{employeeDetail>/JenPel2}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel2Mutation" text="{path: 'employeeDetail>/DescPel2', formatter: '.formatter.formatDescPel'}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel3Mutation" value="{employeeDetail>/JenPel3}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel3Mutation" text="{path: 'employeeDetail>/DescPel3', formatter: '.formatter.formatDescPel'}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel4Mutation" value="{employeeDetail>/JenPel4}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel4Mutation" text="{path: 'employeeDetail>/DescPel4', formatter: '.formatter.formatDescPel'}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="{i18n>actDate}" required="false"/>
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <DatePicker id="actDateStartMutation" value="{employeeDetail>/StartDate}" width="87%" class="sapUiTinyMarginBeginEnd" editable="false"/>
                                <Text text="-" class="sapUiTinyMarginEnd"/>
                                <DatePicker id="actDateEndMutation" value="{employeeDetail>/EndDate}" width="87%" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="information" visible="false"/>
                        <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop" width="57%">
                        <MessageStrip
                            text="Yang bersangkutan sedang dalam proses pengajuan [ST/TP] yang diajukan padaa tanggal dd.mm.yyyy"
                            type="Warning"
                            showIcon="true"
                            showCloseButton="false"
                            class="sapUiMediumMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>    
        

        <!-- Action Request Panel -->
        <Panel 
            expandable="true" 
            expanded="true"
            headerText="Action Request" 
            width="auto">
            <content>
                <f:SimpleForm 
                    id="actionMutationForm"
                    editable="true"
                    layout="ResponsiveGridLayout"
                    labelSpanXL="3"
                    labelSpanL="3"
                    labelSpanM="3"
                    labelSpanS="12"
                    emptySpanXL="1"
                    emptySpanL="1"
                    emptySpanM="1"
                    emptySpanS="1">
                    <f:content>
                        <Label text="{i18n>actionType}" />
                            <Input id="actionTypeMutation" value="{employeeDetail>/ActionType}">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/ActionTypeDesc}">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>reason}" required="true"/>
                            <Input 
                                id="reasonMutation" 
                                value="{reason>/selectedReason/Key}" 
                                fieldGroupIds="grpValidation" 
                                editable="true"
                                showValueHelp="true" 
                                valueHelpRequest=".handleValueHelpReason" 
                                required="true">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{reason>/selectedReason/Value}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>effectiveDate}" required="true" />
                        <HBox class="sapUiSmallMarginBottom">
                            <DatePicker id="effectiveDateStartMutation" value="" required="true" width="95%"/>
                            <Text text="-" class="sapUiSmallMarginBegin sapUiTinyMarginEnd"/>
                            <DatePicker id="effectiveDateEndMutation" value="" width="95%" class="sapUiSmallMarginBegin" editable="false"/>
                        </HBox>
                            
                        <Label text="{i18n>mutation}" visible="false"/>
                            <Text text="Asal"/>
                            <Text text="Tujuan"/>
                        <Label text="{i18n>positionId}" required="true" />
                            <Input id="currentPositionIdMutation" value="{employeeDetail>/EmployeePositionId}" editable="false">
                            <layoutData>
                                <l:GridData span="L1 M1 S12" />
                            </layoutData>
                            </Input> 
                            <Text text="{employeeDetail>/EmployeePositionLongtext}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>                       
                            <Input 
                                id="newPositionIdMutation" 
                                value="{position>/selectedPosition/Key}" 
                                fieldGroupIds="grpValidation" 
                                showValueHelp="true" 
                                liveChange=".onBlockInput"
                                valueHelpRequest=".handleValueHelpPosition" 
                                required="true">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{position>/selectedPosition/Value}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>employeeGroupId}" required="true" />
                            <Input id="currentEmployeeGroupIdMutation" value="{employeeDetail>/StatusAsal}" editable="false">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/EmployeeGrpDesc}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                            <Input 
                                id="newEmployeeGroupIdMutation" 
                                value="{group>/selectedGroup/Key}"
                                fieldGroupIds="grpValidation"
                                editable="true"
                                showValueHelp="true" 
                                liveChange=".onBlockInput"
                                valueHelpRequest=".handleValueHelpGroup" 
                                required="true">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{group>/selectedGroup/Value}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>employeeSubgroupId}" required="true" />
                            <Input id="currentEmployeeSubgroupIdMutation" value="{employeeDetail>/JbtnAsl}" editable="false">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/EmployeeSubgrpDesc}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                            <Input 
                                id="newEmployeeSubgroupIdMutation" 
                                value="{subGroup>/selectedSubGroup/Key2}"
                                fieldGroupIds="grpValidation" 
                                editable="true"
                                showValueHelp="true" 
                                liveChange=".onBlockInput"
                                valueHelpRequest=".handleValueHelpSubGroup" 
                                required="true">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{subGroup>/selectedSubGroup/Value}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>perAreaId}" required="true"/>
                            <Input id="currentPerAreaIdMutation" value="{employeeDetail>/PersonnelAreaId}" editable="false">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/PersonnelAreaText}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                            <Input 
                                id="newPerAreaIdMutation" 
                                value="{area>/selectedArea/Key}"
                                fieldGroupIds="grpValidation"  
                                editable="true"
                                showValueHelp="true" 
                                liveChange=".onBlockInput"
                                valueHelpRequest=".handleValueHelpArea" 
                                required="true">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{area>/selectedArea/Value}" wrapping="true" width="100%"> 
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>perSubAreaId}" required="true"/>
                            <Input id="currentPerSubAreaIdMutation" value="{employeeDetail>/PersonnelSubareaId}" editable="false">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/PersonnelSubareaText}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                            <Input 
                                id="newPerSubAreaIdMutation" 
                                value="{subArea>/selectedSubArea/Key2}" 
                                fieldGroupIds="grpValidation"
                                editable="true"
                                showValueHelp="true" 
                                liveChange=".onBlockInput"
                                valueHelpRequest=".handleValueHelpSubArea" 
                                required="true">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{subArea>/selectedSubArea/Value}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>unitOrgId}"/>
                            <Input id="currentUnitOrgIdMutation" value="{employeeDetail>/EmployeeOrgunitId}" editable="false">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/EmployeeOrgunitLongtext}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                            <Input id="newUnitOrgIdMutation" value="{employeeDetail>/EmployeeOrgunitId}" editable="false">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/EmployeeOrgunitLongtext}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>divisionId}"/>
                            <Input id="currentDivisionIdMutation" value="{employeeDetail>/DivisionId}" editable="false">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/DivisionText}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                            <Input id="newDivisionIdMutation" value="{employeeDetail>/DivisionId}" editable="false">
                                <layoutData>
                                    <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeeDetail>/DivisionText}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L3 M3 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>positionMPP}"/>
                            <Input id="currentPositionMPPMutation" value="{employeeDetail>/Mpp}" editable="false">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Input>
                            <Text text="">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Text>
                            <Input id="newPositionMPPMutation" value="{employeeDetail>/Mpp}" editable="false">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Input>
                        <Label text="{i18n>grade}"/>
                            <Input id="currentGradeMutation" value="{employeeDetail>/Grade}" editable="false">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Input>
                            <Text text="">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Text>
                            <Input id="newGradeMutation" value="{employeeDetail>/Grade}" editable="false">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Input>
                        <Label text="{i18n>endDateJob}"/>
                            <Input id="EndDateMutation" value="{path: 'employeeDetail>/ZENDDA', formatter: '.formatter.formatDateDisplay'}" editable="false">
                            <layoutData>
                                <l:GridData span="L2 M2 S12" />
                            </layoutData>
                            </Input>
                        <Label text="{i18n>salaryAdj}" required="true"/>
                            <Select id="salaryAdjMutation" required="true" fieldGroupIds="grpValidation"
                                items="{ path: 'dropdown>/salaryAdjColl', sorter: { path: 'text' }}"
                                forceSelection="false"
                                selectedKey="{dropdown>/selectedSalaryAdj}">
                                <items>
                                    <core:Item key="{dropdown>key}" text="{dropdown>text}" />
                                </items>
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Select>
                        <Label text="{i18n>salaryAdjValue}"/>
                            <Input 
                                id="salaryAdjValueMutation" 
                                value="{
                                path: 'create>/salaryAdj',
                                type: 'sap.ui.model.type.Currency',
                                formatOptions: {
                                    showMeasure: false,
                                    currencyCode: 'IDR',
                                    useSymbol: false,
                                    minFractionDigits: 0,
                                    maxFractionDigits: 0
                                    }
                                }"
                                placeholder="0">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Input>
                            <Text text="IDR">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Text>
                        <Label text="{i18n>as}" required="true"/>
                            <Select id="asMutation" required="true" fieldGroupIds="grpValidation"
                                items="{ path: 'dropdown>/asColl', sorter: { path: 'text' }}"
                                forceSelection="false"
                                selectedKey="{dropdown>/selectedAs}">
                                <items>
                                    <core:Item key="{dropdown>key}" text="{dropdown>text}" />
                                </items>
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Select>
                        <Label text="{i18n>employeeChange}"/>
                            <Input 
                                id="employeeChangeMutation" 
                                value="{employeechange>/selectedEmployeeChange/Key}" 
                                fieldGroupIds="grpValidation"
                                showValueHelp="true" 
                                liveChange=".onBlockInput"
                                valueHelpRequest=".handleValueHelpEmployeeChange">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Input>
                            <Text text="{employeechange>/selectedEmployeeChange/Value}" wrapping="true" width="100%">
                                <layoutData>
                                    <l:GridData span="L2 M2 S12" />
                                </layoutData>
                            </Text>
                            
                        <Label text="{i18n>basicCon}"/>
                            <TextArea 
                                id="basicConMutation"
                                value="" 
                                rows="4"
                                showExceededText="true" 
                                maxLength="255" 
                                liveChange="handleLiveChange" 
                                valueLiveUpdate="true"
                                editable="true">
                                <layoutData>
                                    <l:GridData span="L6 M6 S12" />
                                </layoutData>
                            </TextArea>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginBegin sapUiSmallMarginTop" width="62%">
                            <MessageStrip
                                text="{i18n>warningEvaluation}"
                                type="Information"
                                showIcon="true"
                                class="sapUiTinyMarginBottom">
                            </MessageStrip>
                            <MessageStrip
                                text="{i18n>warningPKWT}"
                                type="Information"
                                showIcon="true"
                                class="sapUiMediumMarginBottom">
                            </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Verification Panel -->
        <Panel id="attCTPanel" expandable="true" expanded="true" headerText="{i18n>upDoc}" width="auto">
                <f:SimpleForm id="attDataForm" editable="true" layout="ResponsiveGridLayout" labelSpanL="1" emptySpanL="-1" columnsL="1" labelSpanM="1" emptySpanM="-1" adjustLabelSpan="false" singleContainerFullSize="false">
                    <f:content>
                    <Label />
                        <upload:UploadSet
                            id="idUploadSet"
                            instantUpload="false"
                            terminationEnabled="false"
                            maxFileSize="10"
                            uploadUrl="/sap/opu/odata/sap/ZHR_MOVEMENT_MAN_SRV/FileAttachmentSet"
                            
                            fileSizeExceeded=".onFileSizeExceed"
                            afterItemAdded=".onAfterItemAdded"
                            afterItemRemoved=".onAfterItemRemoved"
                        >
                            <upload:items>
                                <upload:UploadSetItem fileName="{fileAttachment>FileName}" mediaType="{fileAttachment>FileType}" url="{fileAttachment>Url}">
                                    <upload:customData>
                                        <core:CustomData key="RequestId" value="{fileAttachment>Reqid}" />
                                        <core:CustomData key="Seqnr" value="{fileAttachment>Seqnr}" />
                                    </upload:customData>
                                </upload:UploadSetItem>
                            </upload:items>
                        </upload:UploadSet>
                        <Label />
                        <ObjectStatus id="objAttLimit" class="sapUiSmallMarginBottom" text="{i18n>attFileSizeLimitMsg}" icon="sap-icon://message-information" state="Information" />
                    </f:content>
                </f:SimpleForm>
        </Panel>   
        </semantic:content>
    <semantic:mainAction>
        <semantic:MainAction id="btnSubmitMutation" text="Submit" press="onSendRequest"/>
    </semantic:mainAction>
    </semantic:FullscreenPage>
</mvc:View>