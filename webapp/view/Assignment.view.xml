<mvc:View
   controllerName="bsim.hcmapp.man.movement.controller.Assignment"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns="sap.m"
   xmlns:f="sap.ui.layout.form"
   xmlns:l="sap.ui.layout"
   xmlns:core="sap.ui.core"
   xmlns:semantic="sap.m.semantic">
<semantic:FullscreenPage navButtonPress="onNavBack" showNavButton="true" title="{i18n>employeeAssignment}">
    <semantic:content>
    <VBox class="sapUiSmallMargin">
        <!-- Employee Data Panel -->
        <Panel 
        expandable="true" 
        expanded="true"
        headerText="Employee Data" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                id="employeeAssignmentForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="{i18n>name}"/>
                            <HBox>
                                <Input value="{Assignment>employee>/EmployeeName/FormattedName}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>entryDate}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA1}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA2}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>ratingPA3}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                        <Label text="{i18n>marketingAvg}"/>
                            <HBox>
                                <Input value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                            </HBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Sanksi Aktif Panel -->
        <Panel 
        expandable="true" 
        expanded="true"
        headerText="Sanksi Aktif" 
        width="auto" 
        class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                id="sanksiAssignmentForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12">
                    <f:content>
                        <Label text="{i18n>sanctionType}"/>
                            <l:Grid vSpacing="0" containerQuery="true">
                            <Input id="sanctionTypeAssignment" value="" width="auto" editable="false" >
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <VBox>
                            <Text id="sanctionDescriptionAssignment" text="Surat Teguran Surat" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                                <Button
                                    icon="sap-icon://document-text" 
                                    text="Show Document"
                                    type="Emphasized"
                                    tooltip="View Document"
                                    press="onDisplayDocumentWarning" class="sapUiTinyMarginBegin"/>
                            </VBox>
                            </l:Grid>
                        <Label text="Jenis Pelanggaran" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel1Assignment" value="{Assignment>employee>/JenPel1}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel1Assignment" text="{Assignment>employee>/DescPel1}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel2Assignment" value="{Assignment>employee>/JenPel2}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel2Assignment" text="{Assignment>employee>/DescPel2}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel3Assignment" value="{Assignment>employee>/JenPel3}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel3Assignment" text="{Assignment>employee>/DescPel3}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="" />
                            <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                            <Input id="jenPel4Assignment" value="{Assignment>employee>/JenPel4}" width="auto" editable="false">
                                <layoutData>
                                <l:GridData span="L1 M1 S12" />
                                </layoutData>
                            </Input>
                            <Text id="descPel4Assignment" text="{Assignment>employee>/DescPel4}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                <layoutData>
                                <l:GridData span="L11 M11 S12" />
                                </layoutData>
                            </Text>
                            </l:Grid>
                        <Label text="{i18n>actDate}" required="false"/>
                            <HBox class="sapUiTinyMarginBeginEnd">
                                <DatePicker id="actDateStartAssignment" value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                                <Text text="-" class="sapUiTinyMarginEnd"/>
                                <DatePicker id="actDateEndAssignment" value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                            </HBox>
                        <Label text="information" visible="false"/>
                        <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop" width="57%">
                        <MessageStrip
                            text="Yang bersangkutan sedang dalam proses pengajuan [ST/TP] yang diajukan padaa tanggal dd.mm.yyyy"
                            type="Warning"
                            showIcon="true"
                            showCloseButton="false"
                            class="sapUiMediumMarginBottom">
                        </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Action Request Panel -->
        <Panel 
            expandable="true" 
            expanded="true"
            headerText="Action Request" 
            width="auto" 
            class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                    id="actionAssignmentForm"
                    editable="true"
                    layout="ResponsiveGridLayout"
                    labelSpanXL="4"
                    labelSpanL="4"
                    labelSpanM="4"
                    labelSpanS="12">
                    <f:content>
                        <Label text="{i18n>actionType}" />
                        <HBox class="sapUiTinyMarginBeginEnd">
                            <Input id="actionTypeAssignment" value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false"/>
                        </HBox>
                        <Label text="{i18n>reason}" />
                        <HBox class="sapUiTinyMarginBeginEnd">
                            <Input id="reasonAssignment" value="" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="true"/>
                        </HBox>
                        <Label text="{i18n>effectiveDate}" />
                        <HBox class="sapUiTinyMarginBeginEnd">
                            <DatePicker id="effectiveDateStartAssignment" value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                            <Text text="-" class="sapUiTinyMarginEnd"/>
                            <DatePicker id="effectiveDateEndAssignment" value="" width="87%" class="sapUiTinyMarginBeginEnd"/>
                        </HBox>
                        <Label text="{i18n>mutation}" visible="false"/>
                        <HBox class="sapUiSmallMarginBeginEnd">
                            <Text text="Dari" class="sapUiLargeMarginEnd"/>
                            <Text id="mutationFromAssignment" text="" class="sapUiLargeMarginBeginEnd" />
                            <Text id="mutationToAssignment" text="" class="sapUiTinyMarginBeginEnd" />
                            <Text text="Menjadi" class="sapUiLargeMarginBegin"/>
                        </HBox>
                        <Label text="{i18n>positionId}" required="true" />
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPositionIdAssignment" value="{Assignment>employee>/EmployeePositionId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPositionIdAssignment" value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="{i18n>positionText}" required="true" />
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPositionTextAssignment" value="{Assignment>employee>/EmployeePositionText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPositionTextAssignment" value="{Assignment>employee>/EmployeePositionText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>perAreaId}" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPerAreaIdAssignment" value="{Assignment>employee>/PersonnelAreaId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPerAreaIdAssignment" value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="{i18n>perAreaText}" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPerAreaTextAssignment" value="{Assignment>employee>/PersonnelAreaText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPerAreaTextAssignment" value="{Assignment>employee>/PersonnelAreaText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>perSubAreaId}" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPerSubAreaIdAssignment" value="{Assignment>employee>/PersonnelSubareaId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPerSubAreaIdAssignment" value="" width="85%" class="sapUiLargeMarginBegin" editable="true"/>
                            </HBox>
                        <Label text="{i18n>perSubAreaText}" required="true"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPerSubAreaTextAssignment" value="{Assignment>employee>/PersonnelSubareaText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPerSubAreaTextAssignment" value="{Assignment>employee>/PersonnelSubareaText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>unitOrgId}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentUnitOrgIdAssignment" value="{Assignment>employee>/EmployeeOrgunitId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newUnitOrgIdAssignment" value="{Assignment>employee>/EmployeeOrgunitId}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>unitOrgText}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentUnitOrgTextAssignment" value="{Assignment>employee>/EmployeeOrgunitText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newUnitOrgTextAssignment" value="{Assignment>employee>/EmployeeOrgunitText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>divisionId}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentDivisionIdAssignment" value="{Assignment>employee>/DivisionId}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newDivisionIdAssignment" value="{Assignment>employee>/DivisionId}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>divisionText}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentDivisionTextAssignment" value="{Assignment>employee>/DivisionText}" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newDivisionTextAssignment" value="{Assignment>employee>/DivisionText}" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>positionMPP}"/>
                        <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentPositionMPPAssignment" value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newPositionMPPAssignment" value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                        </HBox>
                        <Label text="{i18n>grade}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="currentGradeAssignment" value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                            <Input id="newGradeAssignment" value="" width="85%" class="sapUiLargeMarginBegin" editable="false"/>
                            </HBox>
                        <Label text="{i18n>endDateJob}"/>
                        <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="EndDateAssignment" value="" width="100%" class="sapUiTinyMarginEnd" editable="false"/>
                        </HBox>
                        <Label text="{i18n>salaryAdj}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Select id="salaryAdjAssignment" required="true" fieldGroupIds="grpValidation"
                                items="{ path: 'dropdown>/salaryAdjColl', sorter: { path: 'text' }}"
                                forceSelection="false"
                                selectedKey="{dropdown>/selectedSalaryAdj}"
                                width="11.5rem">
                                <items>
                                    <core:Item key="{dropdown>key}" text="{dropdown>text}" />
                                </items>
                            </Select>
                            </HBox>
                        <Label text="{i18n>salaryAdjValue}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="salaryAdjValueAssignment" value="" width="100%" class="sapUiTinyMarginEnd" editable="true"/>
                            </HBox>
                        <Label text="{i18n>as}" required="true"/>
                            <Select id="asAssignment" required="true" fieldGroupIds="grpValidation"
                                items="{ path: 'dropdown>/asColl', sorter: { path: 'text' }}"
                                forceSelection="false"
                                selectedKey="{dropdown>/selectedAs}"
                                width="11.5rem"
                                class="sapUiSmallMarginBegin">
                                <items>
                                    <core:Item key="{dropdown>key}" text="{dropdown>text}" />
                                </items>
                            </Select>
                        <Label text="{i18n>employeeChange}"/>
                            <HBox class="sapUiSmallMarginBeginEnd">
                            <Input id="employeeChangeAssignment" value="" width="100%" class="sapUiTinyMarginEnd" editable="true"/>
                            </HBox>
                        <Label text="{i18n>basicCon}" required="true"/>
                        <VBox class="sapUiSmallMarginBeginEnd">
                            <TextArea 
                                id="basicConAssignment"
                                value="" 
                                width="57%"
                                rows="4"
                                showExceededText="true" 
                                maxLength="255" 
                                liveChange="handleLiveChange" 
                                valueState="Warning" 
                                valueLiveUpdate="true"
                                required="true" 
                                fieldGroupIds="grpValidation"
                                class="sapUiTinyMarginEnd" 
                                editable="true"/>
                        </VBox>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginBegin sapUiSmallMarginTop" width="57%">
                            <MessageStrip
                                text="{i18n>warningEvaluation}"
                                type="Information"
                                showIcon="true"
                                class="sapUiTinyMarginBottom">
                            </MessageStrip>
                            <MessageStrip
                                text="{i18n>warningPKWT}"
                                type="Information"
                                showIcon="true"
                                class="sapUiMediumMarginBottom">
                            </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        <!-- Verification Panel -->
        <Panel 
            expandable="true" 
            expanded="false"
            headerText="Verification" 
            width="auto" 
            class="sapUiResponsiveMargin">
            <content>
                <f:SimpleForm 
                    editable="true"
                    id="assessmentAssignmentForm"
                    layout="ResponsiveGridLayout" 
                    labelSpanXL="4"
                    labelSpanL="4"
                    labelSpanM="4"
                    labelSpanS="12">
                    <f:content>
                        <Toolbar id="AssessmentAssignment">
                            <Text text="{i18n>assessment}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="{i18n>assessmentResult}" required="true" fieldGroupIds="grpValidation"/>
                            <HBox>
                                <Input id="assessmentResultAssignment" value="No data, harap upload hasil assessment" width="18rem" class="sapUiMediumMarginEnd" editable="false"/>
                                <CheckBox id="verifyResultAssignment" text="Verify" selected="{cbResultAssignment>/verifyResultAssignment}"/>
                            </HBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                    editable="true"
                    id="biCheckingAssignmentForm"
                    layout="ResponsiveGridLayout" 
                    labelSpanXL="4"
                    labelSpanL="4"
                    labelSpanM="4"
                    labelSpanS="12">
                    <f:content>
                        <Toolbar id="BICheckingAssignment">
                            <Text text="{i18n>biChecking}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="{i18n>biResult}" required="true" fieldGroupIds="grpValidation"/>
                        <HBox class="sapUiSmallMarginEnd">
                            <Text text="Collection:" class="sapUiSmallMarginEnd" />
                            <CheckBox id="hasilAssignment1" text="1" selected="{hasilAssignment>/hasilAssignment1}"/>
                            <CheckBox id="hasilAssignment2" text="2" selected="{hasilAssignment>/hasilAssignment2}"/>
                            <CheckBox id="hasilAssignment3" text="3" selected="{hasilAssignment>/hasilAssignment3}"/>
                            <CheckBox id="hasilAssignment4" text="4" selected="{hasilAssignment>/hasilAssignment4}"/>
                            <CheckBox id="hasilAssignment5" text="5" selected="{hasilAssignment>/hasilAssignment5}"/>
                        </HBox>
                        <Label text="{i18n>biNotes}" />
                        <TextArea 
                            id="hasilBiCheckingAssignment"
                            value=""
                            width="57%"
                            rows="4"
                            showExceededText="true" 
                            maxLength="255" 
                            liveChange="handleLiveChange" 
                            valueState="Warning" 
                            valueLiveUpdate="true"
                            editable="true"/>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginTop" width="57%">
                            <MessageStrip
                                text="{i18n>warningBiChecking}"
                                type="Information"
                                showIcon="true"
                                class="sapUiTinyMarginBottom">
                            </MessageStrip>
                        </VBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                    editable="true"
                    id="dispoAssignmentForm"
                    layout="ResponsiveGridLayout" 
                    labelSpanXL="4"
                    labelSpanL="4"
                    labelSpanM="4"
                    labelSpanS="12">
                    <f:content>
                        <Toolbar id="disposisiToolbarAssignment">
                            <Text text="{i18n>disposisi}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="{i18n>downloadDis}" />
                        <HBox class="sapUiSmallMarginEnd">
                            <Button
                                icon="sap-icon://download"
                                text="Download"
                                type="Emphasized"
                                press="onDownloadDisposisi"
                                class="sapUiTinyMarginEnd"/>
                        </HBox>
                        <Label text="{i18n>disResult}" required="true" fieldGroupIds="grpValidation"/>
                        <RadioButtonGroup id="disposisiAssignment" columns="2" width="100%" selectedIndex="{disposisiStatus>/selectedIndex}">
                            <RadioButton text="Rekomendasi" class="sapUiSmallMarginEnd"/>
                            <RadioButton text="Tidak Direkomendasikan" />
                        </RadioButtonGroup>
                        <Label text="{i18n>dispoNote}" />
                        <TextArea 
                            id="dispoNoteAssignment"
                            value=""
                            width="57%"
                            rows="4"
                            showExceededText="true" 
                            maxLength="255" 
                            liveChange="handleLiveChange" 
                            valueState="Warning" 
                            valueLiveUpdate="true"
                            editable="true"/>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginTop" width="57%">
                            <MessageStrip
                                text="{i18n>warningDisposisi}"
                                type="Information"
                                showIcon="true"
                                class="sapUiTinyMarginBottom">
                            </MessageStrip>
                        </VBox>
                        <Label text="{i18n>salaryAdj}" />
                        <HBox>
                            <Text text="Rp" />
                            <Input 
                                id="gajiAssignment"
                                type="Number"
                                value=""
                                width="85%"
                                maxLength="50"
                                editable="true"/>
                        </HBox>
                    </f:content>
                </f:SimpleForm>
                <f:SimpleForm
                    editable="true"
                    id="upDocAssignmentForm"
                    layout="ResponsiveGridLayout" 
                    labelSpanXL="4"
                    labelSpanL="4"
                    labelSpanM="4"
                    labelSpanS="12">
                    <f:content>
                        <Toolbar id="upDocAssignment">
                            <Text text="{i18n>upDoc}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <VBox class="sapUiSmallMarginEnd">
                            <HBox alignItems="Center" justifyContent="End">
                                <Button
                                    width="5rem"
                                    icon="sap-icon://add"
                                    type="Transparent"
                                    tooltip="Add Item"
                                    press=".onOpenDialog"/>
                            </HBox>
                            <HBox>
                                <Table
                                    id="table-uploadSet"
                                    width="70rem"
                                    mode="MultiSelect"
                                    fixedLayout="false"
                                    inset="true"
                                    autoPopinMode="true"
                                    hiddenInPopin="None"
                                    items="{path: 'docUploads>/uploads', templateShareable: false}"                                
                                    selectionChange="onSelectionChange">
                                    <columns>
                                        <Column id="namaDokumen" importance="High">
                                            <header>
                                                <Label text="Nama Dokumen" />
                                            </header>
                                        </Column>
                                        <Column id="jenisDokumen">
                                            <header>
                                                <Label text="Jenis Dokumen" />
                                            </header>
                                        </Column>
                                        <Column id="pic">
                                            <header>
                                                <Label text="PIC" />
                                            </header>
                                        </Column>
                                        <Column id="keterangan">
                                            <header>
                                                <Label text="Keterangan" />
                                            </header>
                                        </Column>
                                        <Column id="actionButton" importance="High" />
                                    </columns>
                                    <items>
                                        <ColumnListItem
                                            highlight="{= ${documents>uploadState} === 'Complete' ? 'Indication05' : null }">

                                            <!-- Cells with cell template defining content for each column (cells and columns work with 1:1 mapping) -->
                                            <cells>
                                                <HBox>
                                                    <Image
                                                        src="{documents>imageUrl}"
                                                        class="sapMUSTItemImage sapMUSTItemIcon"
                                                        visible="{= ${documents>imageUrl} !== undefined}"/>
                                                    <core:Icon
                                                        src="sap-icon://pdf-attachment"
                                                        color="white"
                                                        class="sapMUSTItemImage sapMUSTItemIcon"/>
                                                    <VBox
                                                        class="sapUiTinyMargin sapUiSmallMarginBegin">
                                                        <Link
                                                            text="{docUploads>fileName}"
                                                            press="onShowDoc"
                                                            class="sapUiTinyMarginBottom"/>
                                                        <Text text="{path: 'docUploads>fileSize', formatter: '.formatter.formatFileSize'}" />
                                                    </VBox>
                                                </HBox>
                                                <Text text="{docUploads>docTypeText}" />
                                                <Text text="{docUploads>pic}" />
                                                <Text
                                                    text="{docUploads>keterangan}" 
                                                    wrapping="true"/>

                                            </cells>
                                        </ColumnListItem>
                                    </items>
                                </Table>
                            </HBox>                  
                        </VBox> 
                    </f:content>
                </f:SimpleForm>
            </content>
        </Panel>
        </VBox>
            </semantic:content>
            <semantic:mainAction>
                <semantic:MainAction id="btnSubmitAssignment" text="Submit" press="onSendRequest"/>
            </semantic:mainAction>
        </semantic:FullscreenPage>
        </mvc:View>