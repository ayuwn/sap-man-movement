<core:FragmentDefinition 
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    stretch="false">
    <SelectDialog
        noDataText="No Group Found"
        title="Select Group"
        search="handleSearchGroup"
        confirm="handleCloseGroup"
        cancel="handleCloseGroup"
        items="{group>/}">
        <StandardListItem
            title="{group>Key}"
            description="{group>Value}"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>