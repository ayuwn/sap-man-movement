<core:FragmentDefinition 
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    stretch="false">
    <SelectDialog
        noDataText="No Tipe Kontrak Found"
        title="Select Tipe Kontrak"
        search="handleSearchTipeKontrak"
        confirm="handleCloseTipeKontrak"
        cancel="handleCloseTipeKontrak"
        items="{tipekontrak>/}">
        <StandardListItem
            title="{tipekontrak>Key}"
            description="{tipekontrak>Value}"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>