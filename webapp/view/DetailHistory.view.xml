<mvc:View
    controllerName="bsim.hcmapp.man.movement.controller.DetailHistory"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:semantic="sap.m.semantic"
    xmlns:upload="sap.m.upload">
    <semantic:FullscreenPage navButtonPress="onNavBack" showNavButton="true" title="{i18n>myrequest}">
        <semantic:content>
            <VBox class="sapUiSmallMargin">
                <!-- Employee Data Panel -->
                <Panel
                    expandable="true"
                    expanded="false"
                    headerText="Employee Data"
                    width="auto"
                    class="sapUiResponsiveMargin">
                    <content>
                        <f:SimpleForm
                            id="employeeHistoryForm"
                            editable="true"
                            layout="ResponsiveGridLayout"
                            labelSpanXL="4"
                            labelSpanL="4"
                            labelSpanM="4"
                            labelSpanS="12">
                            <f:content>
                                <Label text="{i18n>name}" />
                                <HBox>
                                    <Input id="employeeNameHistory" value="{detailHistoryModel>/EmployeeName/FormattedName}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false" />
                                </HBox>
                                <Label text="{i18n>entryDate}" />
                                <HBox>
                                    <Input id="entryDateHistory" value="{path: 'detailHistoryModel>/HiringDate', formatter: '.formatter.formatDateDisplay'}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false" />
                                </HBox>
                                <Label text="{i18n>ratingPA1}" />
                                <HBox>
                                    <Input id="ratingPA1History" value="{detailHistoryModel>/Rat01}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false" />
                                </HBox>
                                <Label text="{i18n>ratingPA2}" />
                                <HBox>
                                    <Input id="ratingPA2History" value="{detailHistoryModel>/Rat02}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false" />
                                </HBox>
                                <Label text="{i18n>ratingPA3}" />
                                <HBox>
                                    <Input id="ratingPA3History" value="{detailHistoryModel>/Rat03}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false" />
                                </HBox>
                                <Label text="{i18n>marketingAvg}" />
                                <HBox>
                                    <Input id="marketingAvgHistory" value="{detailHistoryModel>/Avg}" fieldWidth="auto" class="sapUiTinyMarginBeginEnd" editable="false" />
                                </HBox>
                            </f:content>
                        </f:SimpleForm>
                    </content>
                </Panel>
                <!-- Sanksi Aktif Panel -->
                <Panel
                    expandable="true"
                    expanded="false"
                    headerText="Sanksi Aktif"
                    width="auto"
                    class="sapUiResponsiveMargin">
                    <content>
                        <f:SimpleForm
                            id="sanksiHistoryForm"
                            editable="true"
                            layout="ResponsiveGridLayout"
                            labelSpanXL="4"
                            labelSpanL="4"
                            labelSpanM="4"
                            labelSpanS="12">
                            <f:content>
                                <Label text="{i18n>sanctionType}" />
                                <l:Grid vSpacing="0" containerQuery="true">
                                    <Input id="sanctionTypeHistory" value="{detailHistoryModel>/SanksiAktif}" width="auto" editable="false">
                                        <layoutData>
                                            <l:GridData span="L1 M1 S12" />
                                        </layoutData>
                                    </Input>
                                    <VBox>
                                        <Text id="sanctionDescriptionHistory" text="Surat Teguran Surat" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                            <layoutData>
                                                <l:GridData span="L11 M11 S12" />
                                            </layoutData>
                                        </Text>
                                        <Button
                                            icon="sap-icon://document-text"
                                            text="Show Document"
                                            type="Emphasized"
                                            tooltip="View Document"
                                            press="onDisplayDocumentWarning"
                                            class="sapUiTinyMarginBegin" />
                                    </VBox>
                                </l:Grid>
                                <Label text="Jenis Pelanggaran" />
                                <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                                    <Input id="jenPel1History" value="{detailHistoryModel>/JenPel1}" width="auto" editable="false">
                                        <layoutData>
                                            <l:GridData span="L1 M1 S12" />
                                        </layoutData>
                                    </Input>
                                    <Text id="descPel1History" text="{path: 'detailHistoryModel>/DescPel1', formatter: '.formatter.formatDescPel'}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                        <layoutData>
                                            <l:GridData span="L11 M11 S12" />
                                        </layoutData>
                                    </Text>
                                </l:Grid>
                                <Label text="" />
                                <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                                    <Input id="jenPel2History" value="{detailHistoryModel>/JenPel2}" width="auto" editable="false">
                                        <layoutData>
                                            <l:GridData span="L1 M1 S12" />
                                        </layoutData>
                                    </Input>
                                    <Text id="descPel2History" text="{path: 'detailHistoryModel>/DescPel2', formatter: '.formatter.formatDescPel'}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                        <layoutData>
                                            <l:GridData span="L11 M11 S12" />
                                        </layoutData>
                                    </Text>
                                </l:Grid>
                                <Label text="" />
                                <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                                    <Input id="jenPel3History" value="{detailHistoryModel>/JenPel3}" width="auto" editable="false">
                                        <layoutData>
                                            <l:GridData span="L1 M1 S12" />
                                        </layoutData>
                                    </Input>
                                    <Text id="descPel3History" text="{path: 'detailHistoryModel>/DescPel3', formatter: '.formatter.formatDescPel'}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                        <layoutData>
                                            <l:GridData span="L11 M11 S12" />
                                        </layoutData>
                                    </Text>
                                </l:Grid>
                                <Label text="" />
                                <l:Grid defaultSpan="XL1 L1 M1 S12" vSpacing="0" containerQuery="true">
                                    <Input id="jenPel4History" value="{detailHistoryModel>/JenPel4}" width="auto" editable="false">
                                        <layoutData>
                                            <l:GridData span="L1 M1 S12" />
                                        </layoutData>
                                    </Input>
                                    <Text id="descPel4History" text="{path: 'detailHistoryModel>/DescPel4', formatter: '.formatter.formatDescPel'}" wrapping="true" width="100%" class="sapUiTinyMarginBegin">
                                        <layoutData>
                                            <l:GridData span="L11 M11 S12" />
                                        </layoutData>
                                    </Text>
                                </l:Grid>
                                <Label text="{i18n>actDate}" required="false" />
                                <HBox class="sapUiTinyMarginBeginEnd">
                                    <DatePicker id="actDateStartHistory" value="{detailHistoryModel>/StartDate}" width="92%" class="sapUiTinyMarginBeginEnd" editable="false" />
                                    <Text text="-" class="sapUiTinyMarginEnd" />
                                    <DatePicker id="actDateEndHistory" value="{detailHistoryModel>/EndDate}" width="92%" class="sapUiTinyMarginBeginEnd" editable="false" />
                                </HBox>
                                <Label text="information" visible="false" />
                                <VBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop" width="57%">
                                    <MessageStrip
                                        text="Yang bersangkutan sedang dalam proses pengajuan [ST/TP] yang diajukan padaa tanggal dd.mm.yyyy"
                                        type="Warning"
                                        showIcon="true"
                                        showCloseButton="false"
                                        class="sapUiMediumMarginBottom">
                                    </MessageStrip>
                                </VBox>
                            </f:content>
                        </f:SimpleForm>
                    </content>
                </Panel>
                <!-- Action Request Panel -->
                <Panel
                    expandable="true"
                    expanded="true"
                    headerText="Action Request"
                    width="auto"
                    class="sapUiResponsiveMargin">
                    <content>
                        <f:SimpleForm
                            id="actionHistoryForm"
                            editable="true"
                            layout="ResponsiveGridLayout"
                            labelSpanXL="4"
                            labelSpanL="4"
                            labelSpanM="4"
                            labelSpanS="12">
                            <f:content>
                                <Label text="{i18n>actionType}" />
                                <HBox class="sapUiTinyMarginBeginEnd">
                                    <Input id="actionTypeHistory" value="{detailHistoryModel>/ActionType}" width="auto" class="sapUiTinyMarginBegin" editable="false" />
                                    <Text text="{detailHistoryModel>/ActionTypeDesc}" class="sapUiTinyMarginBegin"/>
                                </HBox>
                                <Label text="{i18n>reason}" required="true" />
                                <HBox class="sapUiTinyMarginBeginEnd">
                                    <Input id="reasonHistory" value="{detailHistoryModel>/Massg}" fieldWidth="110%" width="auto" class="sapUiTinyMarginBegin" editable="false" />
                                    <Text text="{detailHistoryModel>/MassgDesc}" class="sapUiTinyMarginBegin"/>
                                </HBox>
                                <Label text="{i18n>effectiveDate}" />
                                <HBox class="sapUiTinyMarginBeginEnd">
                                    <DatePicker id="effectiveDateStartHistory" value="{path: 'detailHistoryModel>/ZbegdaEfktf', formatter: '.formatter.formatDateDisplay'}" width="auto" class="sapUiTinyMarginBegin sapUiMediumMarginEnd" editable="false" />
                                    <Text text="-" />
                                    <DatePicker id="effectiveDateEndHistory" value="{path: 'detailHistoryModel>/ZenddaEfktf', formatter: '.formatter.formatDateDisplay'}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>mutation}" visible="false"/>
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Text text="Dari" class="sapUiSmallMarginEnd"/>
                                    <Text id="mutationFromHistory" text="" class="sapUiLargeMarginBeginEnd" />
                                    <Text id="mutationToHistory" text="" class="sapUiMediumMarginBeginEnd" />
                                    <Text text="Menjadi" class="sapUiLargeMarginBegin"/>
                                </HBox>
                                <Label text="{i18n>positionId}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentPositionIdHistory" value="{detailHistoryModel>/EmployeePositionId}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newPositionIdHistory" value="{detailHistoryModel>/PlansDest}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>positionText}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentPositionTextHistory" value="{detailHistoryModel>/EmployeePositionText}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newPositionTextHistory" value="{detailHistoryModel>/PlansDesc_Dest}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>employeeGroupId}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentEmployeeGroupIdHistory" value="{detailHistoryModel>/StatusAsal}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newEmployeeGroupIdHistory" value="{detailHistoryModel>/PersgDest}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>employeeGroupText}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentEmployeeGroupTextHistory" value="{detailHistoryModel>/EmployeeGrpDesc}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newEmployeeGroupTextHistory" value="{detailHistoryModel>/PersgDestDesc}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>employeeSubgroupId}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentEmployeeSubgroupIdHistory" value="{detailHistoryModel>/JbtnAsl}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newEmployeeSubgroupIdHistory" value="{detailHistoryModel>/PerskDest}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>employeeSubgroupText}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentEmployeeSubgroupTextHistory" value="{detailHistoryModel>/EmployeeSubgrpDesc}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newEmployeeSubgroupTextHistory" value="{detailHistoryModel>/PerskDestDesc}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>perAreaId}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentPerAreaIdHistory" value="{detailHistoryModel>/PersonnelAreaId}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newPerAreaIdHistory" value="{detailHistoryModel>/WerksDest}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>perAreaText}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentPerAreaTextHistory" value="{detailHistoryModel>/PersonnelAreaText}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newPerAreaTextHistory" value="{detailHistoryModel>/WerksDestDesc}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>perSubAreaId}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentPerSubAreaIdHistory" value="{detailHistoryModel>/PersonnelSubareaId}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newPerSubAreaIdHistory" value="{detailHistoryModel>/BtrtlDest}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>perSubAreaText}" required="true" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentPerSubAreaTextHistory" value="{detailHistoryModel>/PersonnelSubareaText}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newPerSubAreaTextHistory" value="{detailHistoryModel>/BtrtlDestDesc}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>unitOrgId}" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentUnitOrgIdHistory" value="{detailHistoryModel>/EmployeeOrgunitId}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newUnitOrgIdHistory" value="{detailHistoryModel>/EmployeeOrgunitId}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>unitOrgText}" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentUnitOrgTextHistory" value="{detailHistoryModel>/EmployeeOrgunitText}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newUnitOrgTextHistory" value="{detailHistoryModel>/EmployeeOrgunitText}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>divisionId}" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentDivisionIdHistory" value="{detailHistoryModel>/DivisionId}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newDivisionIdHistory" value="{detailHistoryModel>/DivisionId}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>divisionText}" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentDivisionTextHistory" value="{detailHistoryModel>/DivisionText}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newDivisionTextHistory" value="{detailHistoryModel>/DivisionText}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>positionMPP}" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentPositionMPPHistory" value="{detailHistoryModel>/Mpp}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newPositionMPPHistory" value="{detailHistoryModel>/Mpp}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>grade}" />
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="currentGradeHistory" value="{detailHistoryModel>/Grade}" width="auto" class="sapUiMediumMarginEnd" editable="false" />
                                    <Input id="newGradeHistory" value="{detailHistoryModel>/Grade}" width="auto" class="sapUiMediumMarginBegin" editable="false" />
                                </HBox>
                                <Label text="{i18n>endDateJob}"/>
                                <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="EndDateHistory" value="{path: 'detailHistoryModel>/ZENDDA', formatter: '.formatter.formatDateDisplay'}" width="auto" class="sapUiMediumMarginEnd" editable="false"/>
                                </HBox>
                                <Label text="{i18n>salaryAdj}" required="true"/>
                                    <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="salaryAdjHistory"
                                    value="{parts: [
                                                { path: 'detailHistoryModel>/Adjstmn' }, 
                                                { path: 'dropdown>/salaryAdjColl' }
                                            ], 
                                            formatter: '.formatter.asTextFormatter'}" 
                                            width="auto" 
                                            class="sapUiMediumMarginEnd" 
                                            editable="false"/>
                                    </HBox>
                                <Label text="{i18n>salaryAdjValue}"/>
                                    <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="salaryAdjValueHistory" value="{path: 'detailHistoryModel>/Zsalary', formatter: '.formatter.formatSalary'}" width="auto" class="sapUiMediumMarginEnd" editable="false" description="IDR"/>
                                    </HBox>
                                <Label text="{i18n>as}" required="true"/>
                                    <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="asHistory" 
                                    value="{parts: [
                                                { path: 'detailHistoryModel>/Zdasar1' }, 
                                                { path: 'dropdown>/asColl' }
                                            ], 
                                            formatter: '.formatter.asTextFormatter'}" 
                                            width="auto" 
                                            class="sapUiTinyMarginEnd" 
                                            editable="false"/>
                                    </HBox>
                                <Label text="{i18n>employeeChange}"/>
                                    <HBox class="sapUiSmallMarginBeginEnd">
                                    <Input id="employeeChangeHistory" value="{detailHistoryModel>/Zexholder}" width="auto" class="sapUiTinyMarginEnd" editable="false"/>
                                    <Text text="{detailHistoryModel>/ZexholderDesc}" />
                                    </HBox>
                                <Label text="{i18n>basicCon}"/>
                                <VBox class="sapUiSmallMarginBeginEnd">
                                    <TextArea 
                                        id="basicConHistory"
                                        value="{detailHistoryModel>/Zdasar2}" 
                                        width="57%"
                                        rows="4"
                                        class="sapUiTinyMarginEnd" 
                                        editable="false"/>
                                </VBox>
                                <Label text="informasi" visible="false"/>
                                <VBox class="sapUiSmallMarginBegin sapUiSmallMarginTop" width="57%">
                                    <MessageStrip
                                        text="{i18n>warningEvaluation}"
                                        type="Information"
                                        showIcon="true"
                                        class="sapUiTinyMarginBottom">
                                    </MessageStrip>
                                    <MessageStrip
                                        text="{i18n>warningPKWT}"
                                        type="Information"
                                        showIcon="true"
                                        class="sapUiMediumMarginBottom">
                                    </MessageStrip>
                                </VBox>
                            </f:content>
                        </f:SimpleForm>
                    </content>
                </Panel>
                <!-- Verification Panel -->
                <Panel 
                    expandable="true" 
                    expanded="true"
                    headerText="Verification" 
                    width="auto" 
                    class="sapUiResponsiveMargin">
                    <content>
                        <f:SimpleForm 
                            editable="true"
                            id="assessmentHistoryForm"
                            layout="ResponsiveGridLayout" 
                            labelSpanXL="4"
                            labelSpanL="4"
                            labelSpanM="4"
                            labelSpanS="12">
                            <f:content>
                                <Toolbar id="AssessmentHistory">
                                    <Text text="{i18n>assessment}" class="sapUiSmallMarginBegin"/>
                                </Toolbar>
                                <Label text="{i18n>assessmentResult}" fieldGroupIds="grpValidation"/>
                                <HBox>
                                    <Input id="assessmentResultHistory" value="No data, harap upload hasil assessment" width="18rem" class="sapUiMediumMarginEnd" editable="false"/>
                                    <CheckBox id="verifyResultHistory" text="Verify" selected="{path: 'detailHistoryModel>/Zverify', formatter: '.formatter.formatCheckboxValue'}" editable="false"/>
                                </HBox>
                            </f:content>
                        </f:SimpleForm>
                        <f:SimpleForm
                            editable="true"
                            id="biCheckingHistoryForm"
                            layout="ResponsiveGridLayout" 
                            labelSpanXL="4"
                            labelSpanL="4"
                            labelSpanM="4"
                            labelSpanS="12">
                            <f:content>
                                <Toolbar id="BICheckingHistory">
                                    <Text text="{i18n>biChecking}" class="sapUiSmallMarginBegin"/>
                                </Toolbar>
                                <Label text="{i18n>biResult}" required="true" fieldGroupIds="grpValidation"/>
                                <HBox class="sapUiSmallMarginEnd">
                                    <Text text="Collection:" class="sapUiSmallMarginEnd" />
                                    <CheckBox id="hasilHistory1" text="1" selected="{parts: [{path: 'detailHistoryModel>/Zbichecking'}, {value: '1'}], formatter: '.formatter.formatCheckboxBIValue'}" editable="false"/>
                                    <CheckBox id="hasilHistory2" text="2" selected="{parts: [{path: 'detailHistoryModel>/Zbichecking'}, {value: '2'}], formatter: '.formatter.formatCheckboxBIValue'}" editable="false"/>
                                    <CheckBox id="hasilHistory3" text="3" selected="{parts: [{path: 'detailHistoryModel>/Zbichecking'}, {value: '3'}], formatter: '.formatter.formatCheckboxBIValue'}" editable="false"/>
                                    <CheckBox id="hasilHistory4" text="4" selected="{parts: [{path: 'detailHistoryModel>/Zbichecking'}, {value: '4'}], formatter: '.formatter.formatCheckboxBIValue'}" editable="false"/>
                                    <CheckBox id="hasilHistory5" text="5" selected="{parts: [{path: 'detailHistoryModel>/Zbichecking'}, {value: '5'}], formatter: '.formatter.formatCheckboxBIValue'}" editable="false"/>
                                </HBox>
                                <Label text="{i18n>biNotes}" />
                                <TextArea 
                                    id="hasilBiCheckingHistory"
                                    value="{detailHistoryModel>/Znotebicheck}"
                                    width="57%"
                                    rows="4"
                                    editable="false"/>
                                <Label text="informasi" visible="false"/>
                                <VBox class="sapUiSmallMarginTop" width="57%">
                                    <MessageStrip
                                        text="{i18n>warningBiChecking}"
                                        type="Information"
                                        showIcon="true"
                                        class="sapUiTinyMarginBottom">
                                    </MessageStrip>
                                </VBox>
                            </f:content>
                        </f:SimpleForm>
                        <f:SimpleForm
                    editable="true"
                    id="dispoHistoryForm"
                    layout="ResponsiveGridLayout" 
                    labelSpanXL="4"
                    labelSpanL="4"
                    labelSpanM="4"
                    labelSpanS="12">
                    <f:content>
                        <Toolbar id="disposisiToolbarHistory">
                            <Text text="{i18n>disposisi}" class="sapUiSmallMarginBegin"/>
                        </Toolbar>
                        <Label text="{i18n>rekomendasiHCM}" required="true"/>
                        <TextArea 
                            id="rekomendasiHCMHistory"
                            value="{detailHistoryModel>/ZrekomHcm}"
                            width="57%"
                            rows="4"
                            class="sapUiSmallMarginBottom"
                            editable="false"/>
                        <Label text="{i18n>downloadDis}" />
                        <HBox class="sapUiSmallMarginEnd">
                            <Button
                                icon="sap-icon://download"
                                text="Download"
                                type="Emphasized"
                                press="onDownloadDisposisi"
                                class="sapUiTinyMarginEnd"/>
                        </HBox>
                        <Label text="{i18n>disResult}" required="true" fieldGroupIds="grpValidation" />
                        <RadioButtonGroup id="disposisiHistory" enabled="false" columns="2" width="100%" selectedIndex="{path: 'detailHistoryModel>/Zdisposisi', formatter: '.formatter.formatDisposisi'}">
                            <RadioButton text="Rekomendasi" class="sapUiSmallMarginEnd"/>
                            <RadioButton text="Tidak Direkomendasikan" />
                        </RadioButtonGroup>
                        <Label text="{i18n>dispoNote}" />
                        <TextArea 
                            id="dispoNoteHistory"
                            value="{detailHistoryModel>/Znotedisp}"
                            width="57%"
                            rows="4"
                            maxLength="255" 
                            editable="false"/>
                        <Label text="informasi" visible="false"/>
                        <VBox class="sapUiSmallMarginTop" width="57%">
                            <MessageStrip
                                text="{i18n>warningDisposisi}"
                                type="Information"
                                showIcon="true"
                                class="sapUiTinyMarginBottom">
                            </MessageStrip>
                        </VBox>
                        <Label text="{i18n>salaryAdj}" />
                        <HBox>
                            <Input 
                                id="gajiHistory"
                                type="Text"
                                value="{path: 'detailHistoryModel>/Zsalaryfnl', formatter: '.formatter.formatSalary'}"
                                width="auto"
                                maxLength="50"
                                description="IDR"
